				float2 texCoord47 = IN.ase_texcoord7.xy * float2( 2,1 ) + float2( 0,0 );
				float2 panner46 = ( mulTime45 * float2( 0,1 ) + texCoord47);
				
				float fresnelNdotV60 = dot( WorldNormal, WorldViewDirection );
				float fresnelNode60 = ( 0.0 + 0.63 * pow( 1.0 - fresnelNdotV60, 2.44 ) );
				
				float time40 = 0.0;
				float2 voronoiSmoothId40 = 0;
				float2 coords40 = panner14 * 1.0;
				float2 id40 = 0;
				float2 uv40 = 0;
				float fade40 = 0.5;
				float voroi40 = 0;
				float rest40 = 0;
				for( int it40 = 0; it40 <5; it40++ ){
				voroi40 += fade40 * voronoi40( coords40, time40, id40, uv40, 0,voronoiSmoothId40 );
				rest40 += fade40;
				coords40 *= 2;
				fade40 *= 0.5;
				}//Voronoi40
				voroi40 /= rest40;
				#ifdef _VERTEXOFFSET_TOGGLE_ON
				float staticSwitch70 = ( voroi40 * _VertOffsetPower );
				#else
				float staticSwitch70 = 0.0;
				#endif
				

				float3 BaseColor = ( saturate( lerpBlendMode68 )).rgb;
				float3 Normal = tex2D( _Dirt_Normals_01, panner46 ).rgb;
				float3 Emission = ( min( fresnelNode60 , _FresnelPower ) * _FresnelColour ).rgb;
				float3 Specular = 0.5;
				float Metallic = _Metallic;
				float Smoothness = _Smoothness;
				float Occlusion = 1;
				float Alpha = staticSwitch70;
				float AlphaClipThreshold = 0.5;
				float AlphaClipThresholdShadow = 0.5;
				float3 BakedGI = 0;
				float3 RefractionColor = 1;
				float RefractionIndex = 1;
				float3 Transmission = 1;
				float3 Translucency = 1;

